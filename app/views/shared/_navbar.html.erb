<nav>
  <ul>
    <li><%= link_to 'Store 🏠', root_path %></li>
  </ul>
  <% if user_signed_in? %>
    <ul>
      <li><strong><%= current_user.email %></strong></li>
      <li><%= link_to "Edit profile", edit_user_registration_path %></li>
      <li><%= link_to 'Close session', destroy_user_session_path, 'data-turbo-method': :delete %></li>
    </ul>
  <% else %>
    <ul>
      <li><%= link_to 'Sign in', new_user_session_path %></li>
      <li><%= link_to "Regiter", new_user_registration_path %></li>
    </ul>
<% end %>
</nav>
